<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" namespace="Editor.BE.Model" assembly="Editor.BE" xmlns="urn:nhibernate-mapping-2.2">
	<class name="ElementType" lazy="true" table="`ELEMENTTYPE`">
		<id name="Elementtypeid" access="property" column="`ELEMENTTYPEID`" length="22">
      <generator class="sequence">
        <param name="sequence">ELEMENTTYPE_SEQ</param>
      </generator>
		</id>
		<property name="Description" column="`DESCRIPTION`" length="100" />
		<set name="Elements"
          cascade="save-update"
          inverse="true"
          lazy="true">
			<key column="`ELEMENTTYPEID`" />
			<one-to-many class="Element" />
		</set>
	</class>
</hibernate-mapping>